<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">

<struts>
  <package name="cn.com.topit.gl.inventory" namespace="/inventory" extends="cn.com.topit.gl">
    <action name="vInventory" class="vInventoryAction">
      <result type="json" name="managerExt"> 
        <param name="root">records</param>  
        <param name="noCache">true</param> 
      </result>
    </action>
    <action name="inventory" class="inventoryAction">
      <result type="json" name="managerExt"> 
        <param name="root">records</param>  
        <param name="noCache">true</param> 
      </result>
    </action>
  </package>
</struts>
