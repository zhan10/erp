<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.com.topit.gl.dao.VisitService" table="visit_service" dynamic-insert="true" dynamic-update="true">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="servicename" type="java.lang.String">
			<column name="serviceName" length="50" not-null="true"/>
		</property>
		<property name="servicetel" type="java.lang.String">
			<column name="serviceTel" length="50" not-null="true"/>
		</property>
	</class>
</hibernate-mapping>
